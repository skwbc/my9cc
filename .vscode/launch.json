    "version": "0.2.0",
    "configurations": [


        {
            "name": "debug 9cc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/9cc",
            "args": ["a = 3; a;"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}