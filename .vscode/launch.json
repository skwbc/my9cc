    "version": "0.2.0",
    "configurations": [



        {
            "name": "debug 9cc",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/9cc",
            "args": ["abc = 3; abc; return abc;"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}